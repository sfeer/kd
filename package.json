{
  "name": "@sfeer/kd",
  "version": "0.1.8",
  "description": "看到，基于ant-design-vue的二次封装",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "doc:build": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts",
    "lib:build": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfeer/kd.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/kd.umd.js",
  "module": "./dist/kd.es.js",
  "exports": {
    ".": {
      "import": "./dist/kd.es.js",
      "require": "./dist/kd.umd.js"
    }
  },
  "keywords": [
    "vue3",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/sfeer/kd/issues"
  },
  "homepage": "https://github.com/sfeer/kd",
  "license": "MIT",
  "dependencies": {
    "ant-design-vue": "3",
    "axios": "0",
    "vue": "3",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "2",
    "less": "4",
    "prettier": "2",
    "typescript": "4",
    "vite": "2",
    "vite-plugin-dts": "1",
    "vite-plugin-md": "0",
    "vue-tsc": "0"
  }
}
